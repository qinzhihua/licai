define("user:page/setting/accountInfo/accountInfo.js",function(require,exports,module){"use strict";var Dialog=require("common:widget/ui/dialog/dialog"),refreshAvatar=require("user:widget/setting/wdg-refresh-avatar/wdg-refresh-avatar"),dialog={cmbcTips:Dialog.modalDialog({width:"721px",height:"423px",content:$("#setting-cmbc-tips"),cssClass:"setting-cmbc-dialog",maskConfig:{cssClass:"cmbc-mask"}}).after("hide",function(){}).after("show",function(){$("#cmbcCloseBtn").on("click",function(){dialog.cmbcTips.hide()})}),withdrawalTips:Dialog.modalDialog({title:"提示",content:$("#setting-user-withdrawal"),cssClass:"white-user-tips",maskConfig:{cssClass:"white-user-tips-mask"}}).after("hide",function(){}).after("show",function(){$("#sure-withdrawal").on("click",function(){dialog.withdrawalTips.hide()})})},accountInfo={init:function(){refreshAvatar.init(),$(".to-create-account-by-cust").on("click",function(){dialog.cmbcTips.show()}),$(".whiteUserUnBindCard").on("click",function(){dialog.withdrawalTips.show()})}};module.exports=accountInfo});